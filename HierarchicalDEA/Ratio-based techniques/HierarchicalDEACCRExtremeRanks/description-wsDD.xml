<?xml version="1.0" encoding="UTF-8"?>
<program_description>
    <program provider="PUT" name="HierarchicalDEA-CCR_extremeRanks" displayName="HierarchicalDEA-CCR_extremeRanks" version="1.0" />
    <documentation>
        <description>Computes the extreme efficiency ranks for the given DMUs (alternatives) using CCR Data Envelopment Analysis Model with hierarchical structure of outputs.</description>
        <contact>
            <![CDATA[Anna Labijak <anna.labijak@cs.put.poznan.pl>]]>
        </contact>
    </documentation>
    <parameters>
        <input id="units" name="units" displayName="units" isoptional="0">
            <documentation>
                <description>A list of alternatives (DMUs).</description>
            </documentation>
            <xmcda tag="alternatives">
                <![CDATA[<alternatives>
                        <alternative>
                            [...]
                        </alternative>
                        [...]
                    </alternatives>]]>
            </xmcda>
        </input>
        <input id="performanceTable" name="performanceTable" displayName="performance" isoptional="0">
            <documentation>
                <description>A list of alternatives (DMUs) performances (on leaf hierarchy criteria).</description>
            </documentation>
            <xmcda tag="performanceTable">
                <![CDATA[<performanceTable>
						<alternativePerformances>
							<alternativeID> [...] </alternativeID>
							<performance>
								<criterionID>
									[...]
								</criterionID>
								<value>[...]</value>
							</performance>
							[...]
						</alternativePerformances>
						[...]
					</performanceTable>]]>
            </xmcda>
        </input>

        <input id="hierarchy" name="hierarchy" displayName="hierarchy" isoptional="0">
            <documentation>
                <description>The hierarchical structure of criteria.</description>
            </documentation>
            <xmcda tag="hierarchy">
                <![CDATA[
                <hierarchy>
                    <node>
                        <criterionID>[...]</criterionID>
                        <node>
                            <criterionID>[...]</criterionID>
                            <node>
                                [...]
                            </node>
                            [...]
                        </node>
                        [...]
                    </node>
                </hierarchy>]]>
            </xmcda>
        </input>
        <input id="weightsLinearConstraints" name="weightsLinearConstraints" displayName="weights constraints" isoptional="defaultFalse">
            <documentation>
                <description>A list of hierarchy criteria linear constraints.</description>
            </documentation>
            <xmcda tag="criteriaLinearConstraints">
                <![CDATA[<criteriaLinearConstraints>
						<constraint>
							<element>
								<criterionID> [...] </criterionID>
								<coefficient>
									[...]
								</coefficient>
							</element>
							[...]
							<operator>[...]</operator>
							<rhs>
								[...]
							</rhs>
						</constraint>
						[...]
					</criteriaLinearConstraints>]]>
            </xmcda>
        </input>
        <input id="methodParameters" name="methodParameters" displayName="parameters">
            <documentation>
                <description>Represents parameters (hierarchyNode).</description>
            </documentation>
            <xmcda tag="methodParameters">
                <![CDATA[
    <methodParameters>
        <parameter id="hierarchyNode">
            <value><label>%1</label></value>
        </parameter>
    </methodParameters>]]>
            </xmcda>
            <gui status="preferGUI">
                <entry id="%1" type="string" displayName="hierarchyNode">
                    <documentation>
                        <description>ID of the hierarchy criterion for which the analysis should be performed.</description>
                    </documentation>
                    <defaultValue>root</defaultValue>
                </entry>
            </gui>
        </input>
        <output id="bestRank" name="bestRank" displayName="best rank">
            <documentation>
                <description>A list of alternatives with computed best rank for each of them.</description>
            </documentation>
            <xmcda tag="alternativesValues">
                <![CDATA[
         <alternativesValues>
						<alternativeValue>
						  <alternativeID> [...] </alternativeID>
						  <values>
                <value>
							  [...]
						    </value>
              </values>
						</alternativeValue>
						[...]
					</alternativesValues>]]>
            </xmcda>
        </output>
        <output id="worstRank" name="worstRank" displayName="worst rank">
            <documentation>
                <description>A list of alternatives with computed worst rank for each of them.</description>
            </documentation>
            <xmcda tag="alternativesValues">
                <![CDATA[
         <alternativesValues>
						<alternativeValue>
						  <alternativeID> [...] </alternativeID>
						    <values>
                  <value>
							    [...]
						      </value>
                </values>
						</alternativeValue>
						[...]
					</alternativesValues>]]>
            </xmcda>
        </output>
        <output id="messages" name="messages" displayName="messages">
            <documentation>
                <description>A list of messages generated by the algorithm.</description>
            </documentation>
            <xmcda tag="methodMessages" />
        </output>
    </parameters>
</program_description>
